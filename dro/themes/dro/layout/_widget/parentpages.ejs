<% if (page.parents) { %>
  <div class="widget-wrap">
    <h3 class="widget-title">Up to</h3>
    <div class="widget">
      <ul>
      <% for (parent in page.parents) { %>
        <li><a href="<%= page.parents[parent] %>"><%= parent %></a></li>
      <% } %>
      </ul>
    </div>
  </div>
<% } %>
